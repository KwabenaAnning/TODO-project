{
  "name": "project-alpha",
  "version": "1.0.0",
  "master": "./src/server.ts",
  "scripts": {
    "dev": "nodemon ./src/server.ts",
    "start": "nodemon dist/server.js",
    "compile": "tsc --watch",
    "build": "tsc",
    "test": "mocha --require ts-node/register 'src/test/**/*.test.ts'",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "migrate:create": "db-migrate create --config database.json -e dev",
    "migrate:up": "db-migrate up --config database.json -e dev",
    "migrate:down": "db-migrate down -c 2000 --config database.json -e dev"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.16",
    "@types/chai-http": "^4.2.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "chai": "^4.0.0",
    "chai-http": "^4.4.0",
    "dotenv": "^16.4.5",
    "env-var": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "mocha": "^10.4.0",
    "node-pg-migrate": "^7.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@bitreel/sql-quest": "^0.0.6",
    "@types/mocha": "^10.0.6",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.5",
    "sinon": "^18.0.0",
    "sinon-chai": "^3.7.0"
  }
